{
  "name": "seattlewebco/wp-zoom",
  "description": "Simple Zoom integration with WordPress makes anything possible",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "David Jensen",
      "email": "david@seattlewebco.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "SeattleWebCo\\WPZoom\\": "src"
    }
  },
  "config": {
    "platform": {
      "php": "8.0.0"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "repositories": [],
  "require": {
    "league/oauth2-client": "^2.6",
    "monolog/monolog": "^1.25"
  },
  "require-dev": {
    "phpcompatibility/phpcompatibility-wp": "*",
    "php-stubs/wordpress-stubs": "^5.4",
    "phpcompatibility/php-compatibility": "^9.3",
    "wp-coding-standards/wpcs": "^2.2",
    "squizlabs/php_codesniffer": "^3.5",
    "automattic/vipwpcs": "^2.2",
    "php-stubs/woocommerce-stubs": "^5.3",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1"
  },
  "scripts": {
    "test": [
      "phpunit"
    ],
    "phpcs": [
      "phpcs -s -p"
    ],
    "phpcs-pre-commit": [
      "phpcs -s -p -n"
    ],
    "phpcbf": [
      "phpcbf -p"
    ],
    "post-install-cmd": [
      "composer dump-autoload -o"
    ],
    "post-update-cmd": [
      "composer dump-autoload -o"
    ]
  },
  "extra": {
    "scripts-description": {
      "test": "Run unit tests",
      "phpcs": "Analyze code with PHP_CodeSniffer",
      "phpcbf": "Fix coding standards warnings/errors automatically with PHP Code Beautifier"
    }
  }
}
